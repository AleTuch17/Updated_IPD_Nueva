def RykerLawNoNoiseMainStrat (mymoves, othermoves, currentRound):
  if currentRound == 0:
    return True
  if currentRound == 1:
    return False
  if currentRound == 2:
    return True
  if currentRound == 3:
    return False
  if currentRound == 4:
    return False
  if currentRound == 5:
    return False
  if currentRound == 6:
    return True
  if currentRound > 6:
    if othermoves[0] == True:
      if othermoves[1] == False:
        if othermoves[2] == True:
          if othermoves[3] == False:
            if othermoves[4] == False:
              if othermoves[5] == False:
                if othermoves[6] == True:
                  return True
    if othermoves[-1]:
        return True
    else:
        return False


