def MaxCohenHandshakeMain(mymoves, othermoves, currentRound):
  pattern = [False, False, False, False, False, True, False]

  if currentRound < 7:
    return pattern[currentRound]
  else:
    if othermoves[:7] == pattern:
      return True
    else:
      if currentRound % 2 == 1:
        return True
      return False


def MaxCohenHandshakeFeeder1(mymoves, othermoves, currentRound):
  pattern = [False, False, False, False, False, True, False]

  if currentRound < 7:
    return pattern[currentRound]
  else:
    if othermoves[:7] == pattern:
      return False
    else:
      return True



