def MaxRocheHandshakeBot(mymoves, othermoves, currentRound):
  pattern = [False, False, False, False, False, True, False, True]

  if currentRound < 8:
    return pattern[currentRound]
  else:
    if othermoves[:8] == pattern:
      return False
    else:
      return True

def MaxRocheHandshakeMain(mymoves, othermoves, currentRound):
  pattern = [False, False, False, False, False, True, False, True]

  if currentRound < 8:
    return pattern[currentRound]
  else:
    if othermoves[:8] == pattern:
      return True
    else:
      #transition into nuke-for-tat if handshake isn't met
      if othermoves [-1]:
        return True
    return False

