def MayaSprostsTwotitFortat(mymoves, othermoves, currentRound):
    if len(othermoves)<3:
        return False
    if len (othermoves) > 8 and othermoves[0] and not othermoves[1] and not othermoves[2] and not othermoves[3] and not othermoves[4] and othermoves[5] and not othermoves[6] and othermoves [7]:
      return True
    if othermoves[-1]:
        return True
    if othermoves [-2]:
        return True
    else:
        return False