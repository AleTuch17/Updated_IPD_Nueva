def MayaSprostsBLTftwt(mymoves, othermoves, currentRound):  
    if len(othermoves) < 1:
      return False
    if len(othermoves) < 2:
      return True
    if len(othermoves) < 5:
      return False
    if len(othermoves) < 6:
      return True
    if len (othermoves) < 8:
      return False
    if len (othermoves) > 8 and othermoves[0] and not othermoves[1] and not othermoves[2] and not othermoves[3] and not othermoves[4] and othermoves[5] and not othermoves[6] and othermoves [7]:
      return True
    if othermoves[-1] and othermoves[-2]:
      return True
    if  othermoves[-1] and othermoves[-2] and othermoves[-3] and othermoves[-4] and othermoves[-5]:
      return False
    if othermoves[-1] and othermoves[-2] and not othermoves[-3] and othermoves[-4] and othermoves[-5] and not othermoves[-6]:
      return False
    else:
        return False
def MayaSprostsTftwt(mymoves, othermoves, currentRound):
    if len(othermoves) < 1:
      return False
    if len(othermoves) < 2:
      return True
    if len(othermoves) < 5:
      return False
    if len(othermoves) < 6:
      return True
    if len (othermoves) < 8:
      return False
    if len (othermoves) > 8 and othermoves[0] and not othermoves[1] and not othermoves[2] and not othermoves[3] and not othermoves[4] and othermoves[5] and not othermoves[6] and othermoves [7]:
      return True
    if othermoves[-1] and othermoves[-2]:
      return True
    else:
        return False
def MayaSprostsfeeder1(mymoves, othermoves, currentRound):
    if len(othermoves) < 1:
      return True
    if len(othermoves) < 5:
      return False
    if len(othermoves)==5:
      return True
    if len(othermoves)==6:
      return False
    if len(othermoves)==7:
      return True
    if len(othermoves)==8:
      return False
    if len(othermoves) > 9 and not othermoves[0] and not othermoves[1] and othermoves[2] and not othermoves[3]and not othermoves[4]and not othermoves[5] and othermoves [6] and not othermoves[7]and not othermoves[8]:
      return True
    else:
      return False
