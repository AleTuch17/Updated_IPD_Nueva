def JasperFernaldEaterGenerousTFT(mymoves, othermoves, currentRound):
    if currentRound == 199:
        return True
    WinBotList = [True, True, True, False, False, True, True, False]
    LoseBotList = [False, False, False, True, False, True, True, False]
    check=True
    for i in range(min(8,currentRound)):
      if(LoseBotList[i] != othermoves[i]):
        check = False
    if (currentRound < 8 and check):
      return WinBotList[currentRound]
    if (check==1):
      return True
    else:
        if currentRound == 0:
          return False
        elif currentRound == 1:
            return True
        else:
            return not othermoves[currentRound - 1]

def JasperFernaldNoNoiseFeeder1(mymoves, othermoves, currentRound):
    WinBotList = [True, True, True, False, False, True, True, False]
    LoseBotList = [False, False, False, True, False, True, True, False]
    Friend=True
    for i in range(min(8,currentRound)):
      if(WinBotList[i] != othermoves[i]):
        Friend = False
    if (currentRound < 8 and Friend):
      return LoseBotList[currentRound]
    elif(Friend):
      return False
    else:
      return True

