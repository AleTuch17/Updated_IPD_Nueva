def JasperFernaldWinBotNoiseReverseAlternator(mymoves, othermoves, currentRound):
    if(currentRound == 199):
      return True
    WinBotList = [True, True, True, False, False, True, True, False]
    LoseBotList = [False, False, False, True, False, True, True, False]
    if (currentRound < 8):
      return WinBotList[currentRound]
    else:
      FirstTen = othermoves[:8]
      RunningTotalLOSE = 0
      RunningTotalWIN = 0
      for i in range(8):
        if (FirstTen[i]!=LoseBotList[i]):
          RunningTotalLOSE += 1
      for i in range(8):
        if (FirstTen[i]!=WinBotList[i]):
          RunningTotalWIN += 1
      if(RunningTotalLOSE <= 2):
        return True
      elif(RunningTotalWIN <= 2):
        return False
      else:
        if currentRound % 2 == 1:
          return True
        return False

def JasperFernaldNoiseFeeder1(mymoves, othermoves, currentRound):
    WinBotList = [True, True, True, False, False, True, True, False]
    LoseBotList = [False, False, False, True, False, True, True, False]
    if (currentRound < 8):
      return LoseBotList[currentRound]
    else:
      FirstTen = othermoves[:8]
      RunningTotal = 0
      for i in range(8):
        if (FirstTen[i]!=WinBotList[i]):
          RunningTotal += 1
      if(RunningTotal <= 2):
        return False
      else:
        return True

