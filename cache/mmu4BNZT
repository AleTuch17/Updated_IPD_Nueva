def TaigaNishidaHandshakeReceiverAndSigmoidProbabiityRetaliator(mymoves, othermoves, currentRound):
    e = 2.7182818284590452353602874713527
    #Handshakes with "Shake" players to gain maximum value
    starter = [True, True, False, True, False, False, True, False]

    if currentRound < 8:
        return starter[currentRound]
    else:
        if othermoves[:8] == [True, False, False, True, False, False, True, False]:
            return True
        elif othermoves[:8] == [True, True, False, True, False, False, True, False]:
            return False
        else:
            # N set to 10
            N = 10
            actual = min(currentRound, N)

            lastFew = othermoves[-actual:]

            rats = lastFew.count(True)

            rat_frac = float(rats) / actual

            if random.random() < (1 / (1 + e ** (-9 * (rat_frac - 0.5))) - 0.5) / (1 - 2 * 1 / (1 + e ** (9 * 0.5))) + 0.5:
                return True
            else:
                return False

def TaigaNishidaHandshakeGiver1(mymoves, othermoves, currentRound):
    #Dummy handshaker that gives Exploiter free rats
    starter = [True, False, False, True, False, False, True, False]

    if currentRound < 8:
        return starter[currentRound]
    else:
        if othermoves[:8] == [True, True, False, True, False, False, True, False]:
            return False
        else:
            return True

