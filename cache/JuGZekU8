def MayaSprostsnukeForFourtats(mymoves, othermoves, currentRound):
    if len(othermoves) == 0:
      return False
    num_of_betray = 0
    for i in othermoves:
      if i:
        num_of_betray += 1
    if num_of_betray > 3: 
      return True
    else:
      return False
def MayaSprostsGoofy(mymoves, othermoves, currentRound):
    if len(othermoves) < 75:
      return True
    if len(othermoves) > 75:
      return False