def VianFeederEater_10(mymoves, othermoves, currentRound):
  if len(othermoves) == 0:
    return True

  if len(othermoves) == 1:
    return False

  if len(othermoves) == 2:
    return True

  if len(othermoves) == 3:
    return True

  if len(othermoves) == 4:
    return True

  if len(othermoves) == 5:
    return False

  if len(othermoves) == 6:
    return True


  if len(othermoves) >= 7 and othermoves[0] == True and othermoves[1] == True and othermoves[2] == True and othermoves[3] == True and othermoves[4] == True and othermoves[5] == True and othermoves[6] == True:
    return True
  if len(othermoves) >= 7 and othermoves[0] == False and othermoves[1] == False and othermoves[2] == False and othermoves[3] == False and othermoves[4] == False and othermoves[5] == False and othermoves[6] == False:
    return True

  if len(othermoves) == 7:
    return False
  if len(othermoves) == 8:
    return False

  if othermoves[-1] == True:
    return True
  return False
