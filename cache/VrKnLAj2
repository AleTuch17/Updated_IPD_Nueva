def VianFeeder_1(mymoves, othermoves, currentRound):

    if len(othermoves) >= 7 and othermoves[0] == True and othermoves[1] == False and othermoves[2] == True and othermoves[3] == True and othermoves[4] == True and othermoves[5] == False and othermoves[6] == True:
        return False
    return True

