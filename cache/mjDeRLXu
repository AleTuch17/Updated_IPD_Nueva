def MariMarhefkaPrimaryBot(mymoves, othermoves, currentRound):
    #Recognizes drone bots, otherwise recognizes potential strategies and plays tit-for-tat or tit-for-two-tats depending on the strategy.
	if len(othermoves) == 0:
		return False
	if len(othermoves) == 1:
		return False
	if len(othermoves) == 2:
		return True
	if len(othermoves) == 3:
		return True
	if len(othermoves) == 4:
		return False
	if len(othermoves) == 5:
		return True
	if len(othermoves) == 6:
		return True
	if len(othermoves) == 7:
		return False
	if not othermoves[0] and not othermoves [1] and othermoves [2] and othermoves [3] and not othermoves[4] and othermoves [5] and othermoves[6] and not othermoves[7]:
		return True
	if len(othermoves) == 199:
		return True
	else:
		if othermoves[-1] and othermoves[-2]:
			return True
		if not othermoves[0] and othermoves [1] and not othermoves [2] and othermoves[3] and not othermoves[4]:
			return True
		if othermoves[0] and not othermoves[1] and othermoves[2] and not othermoves[3] and othermoves[4]:
			return True
		else:
			return False


def MariMarhefkaFeederBot(mymoves, othermoves, currentRound):
    #recognizes primary bot, otherwise plays tit-for-tat
	if len(othermoves) == 0:
		return False
	if len(othermoves) == 1:
		return False
	if len(othermoves) == 2:
		return True
	if len(othermoves) == 3:
		return True
	if len(othermoves) == 4:
		return False
	if len(othermoves) == 5:
		return True
	if len(othermoves) == 6:
		return True
	if len(othermoves) == 7:
		return False
	if not othermoves[0] and not othermoves [1] and othermoves [2] and othermoves [3] and not othermoves[4] and othermoves [5] and othermoves[6] and not othermoves[7]:
		return False
	else:
		if len(othermoves) == 0:
			return False
		if othermoves[-1]:
			return True
		return False

